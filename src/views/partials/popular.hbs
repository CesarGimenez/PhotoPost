<div class="card mb-2 mt-2">
    <div class="card-header bg-dark text-white">
        <h5><i class="fas fa-fire"></i> Mas populares</h5>
    </div>
    <div class="card-body">
        <div class="row">
        {{#each sidebar1.popular}}
            <div class="col-md-6 mb-1 align-items-center">
                <a href="/images/{{filename}}">
                    <img src="../../public/uploads/{{filename}}" alt="" class="w-100 h-100 img-thumbnail">
                </a>
            </div>
        {{/each}}
        </div>
    </div>
</div>